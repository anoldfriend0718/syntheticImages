FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      coke;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   nonuniform List<scalar>


boundaryField
{
    "(inlet|outlet)"
    {
        type            fixedValue;
        value           uniform 0; 
    }

    "(top|bottom)"
    {
        type            cyclic;
    }

    frontAndBack
    {
        type            empty;
    }
}


